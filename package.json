{
  "name": "proximity-service-calculator",
  "version": "1.0.0",
  "description": "A simple TypeScript calculator with CLI interface",
  "main": "dist/index.js",
  "bin": {
    "calc": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
